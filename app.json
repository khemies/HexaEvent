{
  "expo": {
    "name": "HexaEvent",
    "slug": "HexaEvent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hexagone.jpg",
    "splash": {
      "image": "./assets/hexagone.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#934DBE"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    
    "android": {

   "imagepermissions": {
      "permissions": ["CAMERA_ROLL"]
    },

      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
